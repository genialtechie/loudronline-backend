{
  "kind": "collectionType",
  "collectionName": "post_categories",
  "info": {
    "singularName": "post-category",
    "pluralName": "post-categories",
    "displayName": "PostCategory",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "categoryId": {
      "type": "customField",
      "customField": "plugin::field-uuid.uuid"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "inversedBy": "post_categories"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    }
  }
}
